
object Service "<%= @check_name_real %>" {
<% if @repository -%>
  import "satellite-service"
  check_command = "dummy"
  host_name = "<%= @host_name %>"
  zone = "<%= @zone %>"
<% else -%>
  import "generic-service"
  check_command = "<%= @check_command %>"
  host_name = "<%= @host_name %>"
<% @vars.each do |name, value| -%>
  vars.<%= name %> = "<%= value %>"
<% end -%>
<% end -%>
}

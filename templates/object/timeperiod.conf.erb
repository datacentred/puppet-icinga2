
object TimePeriod "<%= @title %>" {
<% if @import -%>
  import "<%= @import %>"
<% end -%>
<% if @display_name -%>
  display_name = "<%= @display_name %>"
<% end -%>
<% if @ranges -%>
  ranges = {
  <%- @ranges.each do |key, value| -%>
    "<%= key %>" = "<%= value %>"
  <%- end -%>
  }
<% end -%>
}

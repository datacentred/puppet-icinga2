
object Service "<%= @title %>" {
<% if @import -%>
  import "<%= @import %>"
<% end -%>
<% if @check_command -%>
  check_command = "<%= @check_command %>"
<% end -%>
<% if @host_name -%>
  host_name = "<%= @host_name %>"
<% end -%>
<% if @zone -%>
  zone = "<%= @zone %>"
<% end -%>
<% if @vars -%>
  <%- @vars.each do |key, value| -%>
  vars.<%= key %> = <%= scope.function_icinga2_config_value([value]) %>
  <%- end -%>
<% end -%>
}

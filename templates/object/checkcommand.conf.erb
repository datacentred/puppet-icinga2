
object CheckCommand "<%= @title %>" {
  import "plugin-check-command"
  command = [ <%= Array(@command).join(', ') %> ]
<% if @timeout -%>
  timeout = <%= @timeout %>
<% end -%>
<% if @arguments -%>
  arguments = <%= scope.function_icinga2_config_value([@arguments]) %>
<% end -%>
<% if @vars -%>
  <%- @vars.each do |key, value| -%>
  vars.<%= key %> = <%= scope.function_icinga2_config_value([value]) %>
  <%- end -%>
<% end -%>
}
